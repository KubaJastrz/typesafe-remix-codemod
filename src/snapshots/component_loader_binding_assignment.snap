import { useLoaderData } from "@remix-run/react";





export default defineRoute({
  loader() {
    return { hello: "world", foo: "bar" };
  },
  Component({ data: { hello, maybe = "not" } }) {
    
    return <h1>{hello}</h1>;
  },
});
